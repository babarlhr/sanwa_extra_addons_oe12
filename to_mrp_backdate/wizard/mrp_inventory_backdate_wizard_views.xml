<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="mrp_inventory_backdate_wizard_form_view"
		model="ir.ui.view">
		<field name="name">mrp.inventory.backdate.wizard.form</field>
		<field name="model">mrp.inventory.backdate.wizard</field>
		<field name="inherit_id"
			ref="to_backdate.abstract_inventory_backdate_wizard_form_view" />
		<field name="mode">primary</field>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='date']" position="after">
				<field name="mrp_order_id" invisible="1" />
			</xpath>
		</field>
	</record>
</odoo>